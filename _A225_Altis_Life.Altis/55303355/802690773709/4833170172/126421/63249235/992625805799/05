if((life_cashCache!=((life_cash/4)+7))||(life_atmcashCache!=((life_atmcash/4)+5))) exitWith{[["-HACKEDMONEY-",player,profileName,format["File: %1 - money tampering attempted! Cash: %2 - Expected cash: %3 | Bank: %4 - Expected bank: %5.",_fnc_scriptName,[life_cash]call life_fnc_numberText,[((life_cashCache-7)*4)]call life_fnc_numberText,[life_atmCash]call life_fnc_numberText,[((life_atmCashCache-5)*4)]call life_fnc_numberText]],"TON_fnc_logIt",2]spawn life_fnc_MP;[[profileName,"Attempting to hack money variable."],"life_fnc_notifyAdmins",-2,false]spawn life_fnc_MP;["MemHackCash",false,false]call compile PreProcessFileLineNumbers"\a3\functions_f\Misc\fn_endMission.sqf";};params[["_medic",objNull,[objNull]],["_hospital",false,[false]],["_admin",false,[false]],["_copMedic",false,[false]],["_nextRevive",-1000,[0]]];if!(life_isDead) exitWith{};life_respawned=true;private _gear=if(playerSide isEqualTo west&&{life_activeSWAT||life_eviSpawn}) then[{life_deadcop_gear},{life_dead_gear}];[_gear]call life_fnc_loadDeadGear;if(life_sidechat&&(playerSide isEqualTo civilian)&&!(player in((radioChannelInfo life_radio_civ) param[3,[]]))) then{[life_sidechat]call life_fnc_manageSidechat;};if(_hospital) then{cutText["You wake up at a hospital, feeling much better.","BLACK FADED"];0 cutFadeOut 6;private _hospitalLocations=if!(call life_fnc_isDominationActive) then{["revive","revive_1","revive_2","revive_3","revive_4"]}else{["getDominationSpawns",SRV_dominationID]call life_fnc_dominationCfg};private _nearestHospital=[_hospitalLocations,life_corpse]call BIS_fnc_nearestPosition;player setPos(markerPos _nearestHospital);player setDir(markerDir _nearestHospital);[["-REVIVE-",player,profileName,format["respawned at the hospital, POS: %1",getPos player]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;}else{player setDir(getDir life_corpse);player setPosASL(visiblePositionASL life_corpse);if(_admin) then{titleText[format["The admin %1 has revived you.",[name _medic]call life_fnc_cleanName],"PLAIN"];if!(isNull(findDisplay 7300)) then{(findDisplay 7300) closeDisplay 0;};}else{if(playerSide isEqualTo civilian&&{player getVariable["ga_lowtime",false]}) then{[format["Due to being a newer player you've only been charged $300 for these services. In the future you will have to pay $%1 for these same services.",[call(life_revive_fee)]call life_fnc_numberText],"basic"]spawn life_fnc_handleMessage;}else{[format["%1 has revived you and a fee of $%2 was taken from your bank account for their services.",[name _medic]call life_fnc_cleanName,[call(life_revive_fee)]call life_fnc_numberText],"basic"]spawn life_fnc_handleMessage;