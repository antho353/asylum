player setVariable["ga_transporting",nil,true];};player action["Eject",vehicle player];player action["GetOut",vehicle player];waitUntil{isNull objectParent player};if(player getVariable["ga_downed",false]) then{player setUnconscious true;player setVelocity[0,0,0];};titleText["You have been pulled out of the vehicle","PLAIN"];titleFadeOut 4;