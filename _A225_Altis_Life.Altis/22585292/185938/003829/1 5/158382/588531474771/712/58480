}else{_spikeStrip attachTo[vehicle player,[0,-5,0]];};detach _spikeStrip;private _dir=getDir(vehicle player);if(isNull objectParent player) then{_spikeStrip setDir(_dir+90);}else{_spikeStrip setDir _dir;};_spikeStrip setPos[getPos _spikeStrip select 0,getPos _spikeStrip select 1,0];_spikestrip hideObjectGlobal false;_asy_dmg setDamage 1;_spikeStrip setVariable["ga_placed",true,true];[[_spikeStrip,600],"TON_fnc_setCleanTime",2]spawn life_fnc_MP;[[_spikeStrip],"TON_fnc_srvSpikeStrip",2]spawn life_fnc_MP;