};};if(_val>_transferBank) exitWith{[["-HACKEDMONEY-",_from,name _from,format["tried to wiretransfer $%1 to %2 (%3) without having enough funds at position %4.",[_val]call life_fnc_numberText,profileName,getPlayerUID player,getPosATL _from]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;};if(_val>999999) exitWith{[["-HACKEDMONEY-",_from,name _from,format["tried to wiretransfer $%1 to %2 (%3), over the allowed limit, at position %4.",[_val]call life_fnc_numberText,profileName,getPlayerUID player,getPosATL _from]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;};if((count _nearATMs) isEqualTo 0) exitWith{[["-HACKEDMONEY-",_from,name _from,format["tried to wiretransfer $%1 to %2 (%3) without an ATM nearby at position %4.",[_val]call life_fnc_numberText,profileName,getPlayerUID player,getPosATL _from]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;};if!([str _val]call life_fnc_isnumber) exitWith{};if((name _from) isEqualTo"") exitWith{};life_atmcash=(round(life_atmcash+_val)) max 0;life_atmcashCache=((life_atmcash/4)+5);[1]call life_fnc_ClupdatePartial;[format["%1 has wire transferred $%2 to you.",(name _from),[_val]call life_fnc_numberText],"basic"]spawn life_fnc_handleMessage;[["-BANKRECEIVE-",player,(profileName),format["was wiretransfered $%1 from %2 (%3).",[_val]call life_fnc_numberText,(name _from),(getPlayerUID _from)]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;