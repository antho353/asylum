params[["_animal",objNull,[objNull]]];if(isNull _animal||alive _animal) exitWith{};if!(isNil{_animal getVariable"ga_skinning"}) exitWith{};_animal setVariable["ga_skinning",true,true];private _meat=switch(typeOf _animal) do{case"Cock_random_F":{"chicken"};case"Hen_random_F":{"chicken"};case"Fin_random_F":{"dog"};case"Alsatian_Random_F":{"dog"};case"Goat_random_F":{"goat"};case"Sheep_random_F":{"sheep"};case"Rabbit_F":{"rabbit"};case"Snake_random_F":{"snake"};