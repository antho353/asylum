};};private _flags=[arms_flag,drug_flag,oil_flag,wongs_flag];private _container=_flags select _index;if(isNil"_container") exitWith{};switch(_container) do{case(oil_flag):{_details ctrlSetStructuredText parseText format["<t size='0.90' align='center'>Rubber Available: %1</t>",oil_flag getVariable["ga_cap_oil",0]];};default{_details ctrlSetStructuredText parseText"<t size='0.90' align='center'>Money goes to gang bank</t>";};};