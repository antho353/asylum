disableSerialization;params["_control","_index"];private _dataArr=parseSimpleArray(_control lbData _index);private _className=_dataArr select 0;private _vehicleColor=[_className,_dataArr select 1]call life_fnc_vehicleColorStr;private _vehicleInfo=[_className]call life_fnc_fetchVehInfo;private _trunkSpace=[_className]call life_fnc_vehicleConfig;private _retrievePrice=[_className,call life_garage_prices]call life_fnc_index;private _sellPrice=[_className,call life_garage_sell]call life_fnc_index;_retrievePrice=if(_retrievePrice isEqualTo-1) then{1000}else{(call(life_garage_prices) select _retrievePrice) select 1;};_sellPrice=if(_sellPrice isEqualTo-1) then{1000}else{(call(life_garage_sell) select _sellPrice) select 1;