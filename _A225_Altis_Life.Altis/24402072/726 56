private _index=randomized_life_group_list findIf{life_my_gang in _x};if(_index isEqualTo-1) exitWith{life_my_gang=objNull;};(randomized_life_group_list select _index) params["_name","_group","_locked","_ownerID","_password"];openMap false;life_lastGroup=group player;if(_ownerID isEqualTo(getPlayerUID player)) then{private _units=units group player;private _newOwner=_units param[_units findIf{!(_x isEqualTo player)},objNull];if!(isNull _newOwner) then{randomized_life_group_list set[_index,[_name,_group,_locked,getPlayerUID _newOwner,_password]];publicVariable"randomized_life_group_list";