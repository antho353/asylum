};asyv_statTotals=-1;[[getPlayerUID player],"TON_fnc_getStatsArr",2]call life_fnc_MP;waitUntil{uiSleep 0.1;!(asyv_statTotals isEqualTo-1)};if(asyv_statTotals isEqualTo[]) exitWith{["We could not find any stats for supplied playerid.","basic"]spawn life_fnc_handleMessage;};if!(asyv_statTotals isEqualTo-1) then{if(_isTitleApp) then{[]call life_fnc_titleMenu}else{[]spawn life_fnc_statsMenu};};