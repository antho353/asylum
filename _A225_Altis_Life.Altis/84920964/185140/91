if(_price>life_cash) then{_priceTag ctrlSetStructuredText parseText format["<t size='0.75'>Price: <t color='#ff0000'>$%1</t>",[(_price)]call life_fnc_numberText];}else{_priceTag ctrlSetStructuredText parseText format["<t size='0.75'>Price: <t color='#8cff9b'>$%1</t>",[(_price)]call life_fnc_numberText];};