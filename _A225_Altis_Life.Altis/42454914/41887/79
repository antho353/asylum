private _item=cursorObject;if(isNull _item) exitWith{};private _index=[typeOf _item,life_placeable]call life_fnc_index;if(_index isEqualTo-1) exitWith{["This item cannot be picked up.","basic"]spawn life_fnc_handleMessage};if(!(_item getVariable['ga_placedItem',false])) exitWith{["This item was not placed by a player and cannot be picked up.","basic"]spawn life_fnc_handleMessage};