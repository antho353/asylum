private _newTrunkInventory=[];_newTrunkInventory=if(_isUp) then{private _newInv=_trunkInv select[0,_lbSelected-1];_newInv append[(_trunkInv select _lbSelected)];_newInv append[(_trunkInv select(_lbSelected-1))];_newInv append(_trunkInv select[_lbSelected+1,(_trunkLength-(_lbSelected+1))]);_newInv;}else{private _newInv=_trunkInv select[0,_lbSelected];_newInv append[(_trunkInv select _lbSelected+1)];_newInv append[(_trunkInv select(_lbSelected))];_newInv append(_trunkInv select[_lbSelected+2,(_trunkLength-(_lbSelected+1))]);_newInv;};if(count _newTrunkInventory isEqualTo 0) exitWith{};if(_isVirtual) then{life_trunk_vehicle setVariable["ga_vinv",[_newTrunkInventory,_trunkWeight]];[life_trunk_vehicle,life_is_virtual]call life_fnc_refreshInventory;}else{life_trunk_vehicle setVariable["ga_pinv",[_newTrunkInventory,_trunkWeight]];[life_trunk_vehicle,life_is_virtual]call life_fnc_refreshInventory;};if(_isUp) then{lbSetCurSel[4502,_lbSelected-1];}else{lbSetCurSel[4502,_lbSelected+1];};life_movedItem=true;