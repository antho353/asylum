private _vehString=(typeOf _vehicle);["Selling vehicle, please wait...","basic"]spawn life_fnc_handleMessage;life_action_in_use=true;private _price2=life_cash+_price;[[player,_vehicle,_price,_price2],"TON_fnc_srvChopShopSell",false,false]spawn life_fnc_MP;closeDialog 0;life_action_in_use=false;["vehchop",1]call life_fnc_statArrUpdate;[["-CHOPSOLD-",player,(profileName),format["chop sold a %1 (%2) for $%3, owned by (%4) at POS: %5",(getText(configFile>>"CfgVehicles">>_vehString>>"displayName")),_plate,[_price]call life_fnc_numberText,_vehOwner,(getPos player)]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;["The chop shop has consumed the vehicle!","basic"]spawn life_fnc_handleMessage;if(_vehicle getVariable["ga_side",""]=="cop") then{if(typeOf _vehicle isEqualTo"C_Hatchback_01_sport_F") then{["chop","cophatchback",1]call life_fnc_handleQuest;};if(typeOf _vehicle isEqualTo"B_Heli_Light_01_F") then{["chop","cophummingbird",1]call life_fnc_handleQuest;};if(typeOf _vehicle isEqualTo"O_MRAP_02_F") then{["chop","copifrit",1]call life_fnc_handleQuest;};};if(_vehicle getVariable["ga_side",""]=="civ") then{if(typeOf _vehicle isEqualTo"O_Heli_Light_02_unarmed_F") then{["chop","civorca",1]call life_fnc_handleQuest;};if(typeOf _vehicle isEqualTo"O_MRAP_02_F") then{["chop","civifrit",1]call life_fnc_handleQuest;};};