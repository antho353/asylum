if!(SRV_HousesSetup) exitWith{["Please wait a bit while houses are being initalized...","basic"]spawn life_fnc_handleMessage;};if(life_houses isEqualTo[]) exitWith{["You have no houses!","basic"]spawn life_fnc_handleMessage;};private _life_houses=[];{private _pos=parseSimpleArray(_x param[0,[0,0,0]]);private _houses=nearestObjects[_pos,["House"],10,true];if(_houses isNotEqualTo[]) then{_life_houses pushBack(_houses param[0,objNull]);};}forEach life_houses;{private _houseObj=_x;private _houseName=getText(configFile>>"CfgVehicles">>typeOf _houseObj>>"displayName");private _nearestLocation=text(nearestLocations[_houseObj,["NameCityCapital","NameCity","NameVillage"],5000]param[0,"Unknown Location"]);if(isNull _houseObj) exitWith{};if((_houseObj getVariable["ga_homeOwner",""]) isNotEqualTo(getPlayerUID player)) exitWith{["You must be the owner of this property to pay the electric bill","basic"]spawn life_fnc_handleMessage;};(_houseObj getVariable["ga_house_electric",[false,0]]) params[["_electricNeedToPay",false,[false]],["_electricPriceToPay",0,[0]]];if(_electricNeedToPay&&(_electricPriceToPay isNotEqualTo 0)) then{private _action=[format["<t align='center' size='0.95'>Do you wish pay your electric bill of $%1? For your %2, which is near %3.</t>",[_electricPriceToPay]call life_fnc_numberText,_houseName,_nearestLocation],format["Asylum City Limits"],"Yes","No"]call life_fnc_guiMessage;