if((typeOf _veh)=="Steerable_Parachute_F") exitWith{};private _SWATlightState=_veh getVariable["ga_SWATLights",false];if(_SWATlightState) then{_veh setVariable["ga_SWATLights",false,true];if(_veh getVariable"ga_lights") then{_veh setVariable["ga_lights",false,true];uiSleep 0.2;[_veh]call life_fnc_sirenLights;};}else{_veh setVariable["ga_SWATLights",true,true];if(_veh getVariable"ga_lights") then{_veh setVariable["ga_lights",false,true];uiSleep 0.2;[_veh]call life_fnc_sirenLights;}else{[_veh]call life_fnc_sirenLights;}};