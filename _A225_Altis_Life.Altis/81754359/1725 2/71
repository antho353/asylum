params[["_caseType","",[""]],"_govData"];switch(_caseType) do{case("recievePardon"):{_govData params[["_governorObj",objNull,[objNull]],["_governorPID","",[""]],["_governorName","",[""]]];if((isNull _governorObj)||(_governorPID isNotEqualTo(SRV_governorData param[0,"GovUID"]))||(_governorName isEqualTo"")) exitWith{};private _pardonUID=getPlayerUID player;private _dialogQuestion=[format["Do you wish to be pardoned by Governor %1 of %2?",_governorName,worldName],"Governor Pardon","Yes","No"]call BIS_fnc_guiMessage;if!(_dialogQuestion) exitWith{[["pardonDoneBackToGov",[profileName,getPlayerUID player,(A_stats_crimes param[0,0]),true]],"life_fnc_govPardon",_governorObj]call life_fnc_MP;};[["-PARDON-",_governorObj,_governorName,format["gov pardoned %1 (%2) with a bounty of $%3.",profileName,(getPlayerUID player),(A_stats_crimes param[0,0])]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;[["pardonDoneBackToGov",[profileName,getPlayerUID player,(A_stats_crimes param[0,0]),false]],"life_fnc_govPardon",_governorObj]call life_fnc_MP;