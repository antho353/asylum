if((call life_adminLevel)<2) exitWith{closeDialog 0;};if(life_adminMode) exitWith{life_adminMode=false;player allowDamage true;systemChat"Admin mode: off";[["-ADMINACTION-",player,profileName,"disabled admin godmode."],"TON_fnc_logIt",2]spawn life_fnc_MP;[]spawn{uiSleep(60*2);if!(life_adminMode) then{player setVariable["ga_aMode",false,true];[["-ADMINACTION-",player,profileName,"admin godmode shutoff."],"TON_fnc_logIt",2]spawn life_fnc_MP;};};};player setVariable["ga_aMode",true,true];player allowDamage false;systemChat"Admin mode: on";disableSerialization;[["-ADMINACTION-",player,profileName,"enabled admin godmode."],"TON_fnc_logIt",2]spawn life_fnc_MP;life_adminMode=true;asyv_hunger=100;asyv_thirst=100;private _asy_dmg=player;_asy_dmg setDamage 0;[]spawn{while{true}do{if!(life_adminMode) exitWith{};private _markers=[];private _marker="";private _color="";waitUntil{uiSleep 0.2;visibleMap};{_marker=createMarkerLocal[format["%1_marker_HAX",_x],getPos _x];_marker setMarkerTypeLocal"Mil_dot";_marker setMarkerTextLocal format["%1 - %2m",name _x,round(player distance _x)];_markers pushBack[_marker,_x];_color=switch(side _x) do{case west:{"colorBLUFOR"};case independent:{"ColorGreen"};default{"colorCivilian"};