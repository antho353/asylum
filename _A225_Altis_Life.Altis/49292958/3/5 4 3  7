}else{[[_from,_item,_val,_unit,false],"life_fnc_giveDiff",_from]spawn life_fnc_MP;};}else{if([true,_item,_val]call life_fnc_handleInv) then{[format["%1 has gave you %2 %3",[name _from]call life_fnc_cleanName,_val,_type],"basic"]spawn life_fnc_handleMessage;if(_item isEqualTo"bank_money") then{private _time=_from getVariable["ga_banktrace",-1000];if(time-_time<400) then{[[getPlayerUID player,"ga_banktrace",_time],"TON_fnc_varPersist",2]spawn life_fnc_MP;};};[["-ITEMRECEIVE-",player,profileName,format["was given %1 %2 from %3 (%4) at POS: %5",_val,_item,name _from,getPlayerUID _from,getPos player]],"TON_fnc_logIt",2,false,false]spawn life_fnc_MP;}else{[[_from,_item,_val,_unit,false],"life_fnc_giveDiff",_from]spawn life_fnc_MP;};};