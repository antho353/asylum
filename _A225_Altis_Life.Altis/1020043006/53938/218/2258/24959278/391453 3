private _uniqueID=if(_indexValZero in[-3,-5]) then{((life_domLoadoutHolder select _indexValOne) select 0)}else{((life_domLoadoutHolder select _indexValZero) select 0)};private _action=[format["Are you sure you want to delete the loadout named %1?",_loadoutName],"Loadout Delete Confirmation","Delete","Cancel"]call BIS_fnc_guiMessage;if!(_action) exitWith{};closeDialog 0;if(_indexValZero in[-3,-5]||(_indexValZero>=0&&_indexValZero<=5)) then{[[1,getPlayerUID player,_uniqueID,playerSide],"TON_fnc_domHandleLoadouts",2]call life_fnc_MP;if(_indexValZero in[-3,-5]) then{life_domLoadoutHolder set[_indexValOne,[]];}else{life_domLoadoutHolder set[_indexValZero,[]];};[format["You have successfully deleted the %1 loadout!",_loadoutName],"basic"]spawn life_fnc_handleMessage;};[["-DOMLOADOUTDEL-",player,(profileName),format["attempted to delete loadout %1 at POS: %2",_uniqueID,getPos player]],"TON_fnc_logIt",2]spawn life_fnc_MP;waitUntil{!dialog};[nil,nil,nil,_type]spawn life_fnc_domLoadoutMenu;