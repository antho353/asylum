};};};}else{switch(_type) do {case 0:{systemChat _msg;};case 1:{[_msg,"basic"]spawn life_fnc_handleMessage;};case 2:{titleText[_msg,"PLAIN DOWN"];};};};};if(_type isEqualType[]) then{for"_i"from 0 to(count _type)-1 do{switch((_type select _i)) do{case 0:{systemChat _message};case 1:{[format["%1",_message],"basic"]spawn life_fnc_handleMessage;};case 2:{titleText[format["%1",_message],"PLAIN DOWN"];};};};}else{switch(_type) do{case 0:{systemChat _message};case 1:{[format["%1",_message],"basic"]spawn life_fnc_handleMessage;};case 2:{titleText[format["%1",_message],"PLAIN DOWN"];};case 3:{["dominationAlert",[format["%1",_message]]]call BIS_fnc_showNotification;};case 4:{["gangFortressAlert",[format["%1",_message]]]call BIS_fnc_showNotification;};};};