if!(_action) exitWith{};if(_val>0) then{[[_val,player,_val,[1]],"life_fnc_clientWireTransfer",_unit,false,false]spawn life_fnc_MP;[["-ADMINACTION-",player,profileName,format["added $%1 to %2's bank account.",_val,name _unit]],"TON_fnc_logIt",2]spawn life_fnc_MP;[format["You added $%1 to %2's bank account.",[_val]call life_fnc_numberText,name _unit]]spawn life_fnc_handleMessage;};};case 2:{life_atmcash=(round(life_atmcash+_val)) max 0;life_atmcashCache=((life_atmcash/4)+5);[["-ADMINACTION-",player,profileName,format["added $%1 to their bank account.",_val]],"TON_fnc_logIt",2]spawn life_fnc_MP;[format["You added $%1 to your bank account.",[_val]call life_fnc_numberText]]spawn life_fnc_handleMessage;};default{};};