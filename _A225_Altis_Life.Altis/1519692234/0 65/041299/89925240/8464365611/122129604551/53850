params["_obj_main","_spawns","_stance",["_type","",[""]]];if(_obj_main isKindOf"CAManBase") then{};switch(_type) do{case"ground":{_obj_main addAction["Ambulance Shop",life_fnc_vehicleShopMenu,["med_car",independent,_spawns,"med","AFD Motorpool"],1.5,false,false,"",'playerSide isEqualTo independent',5];_obj_main addAction["Firefighter Garage",life_fnc_vehicleShopMenu,["firefighter_car",independent,_spawns,"med","Firefighter Garage"],1.5,false,false,"",'playerSide isEqualTo independent && life_isActiveFirefighter',5];_obj_main addAction["Vehicle Garage",{["Car",_this select 3]call life_fnc_handleVehicles;},_spawns,1.5,false,false,"",'playerSide isEqualTo independent',5];_obj_main addAction["Store vehicle in Garage",life_fnc_storeVehicle,"",1.5,false,false,"",'!life_garage_store && playerSide isEqualTo independent',5];};case"air":{_obj_main addAction["Medivac Shop",life_fnc_vehicleShopMenu,["med_air",independent,_spawns,"med","AFD Helicopters"],1.5,false,false,"",'playerSide isEqualTo independent',5];_obj_main addAction["Medivac Garage",{["Air",_this select 3]call life_fnc_handleVehicles;},_spawns,1.5,false,false,"",'playerSide isEqualTo independent',5];_obj_main addAction["Store vehicle in Garage",life_fnc_storeVehicle,"",1.5,false,false,"",'!life_garage_store && playerSide isEqualTo independent',5];