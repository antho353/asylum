}else{deleteMarkerLocal life_gangBldgMark;private _marker=createMarkerLocal[format["phouse_%1%2",_gangID,_newHouse getVariable["ga_homeOwner",""]],getPosATL _newHouse];_marker setMarkerTextLocal"Gang Building";_marker setMarkerColorLocal"ColorGreen";_marker setMarkerTypeLocal"mil_end";life_gangBldgMark=_marker;["Your gang leader has changed the gang house.","basic"]spawn life_fnc_handleMessage;};};