[[_gangID],"TON_fnc_removeGang",2]spawn life_fnc_MP;if!(isNull(life_gangBuilding select 0)) then{(life_gangBuilding select 0) setVariable["ga_house_gang",-1,true];};closeDialog 0;["Disbanding the gang...","shortwarning"]spawn life_fnc_handleMessage;}else{["You have chosen to not disband the gang!","basic"]spawn life_fnc_handleMessage;};