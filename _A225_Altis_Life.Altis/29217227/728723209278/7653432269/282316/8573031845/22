params[["_className","",[""]],["_position",[],[[]]],["_direction",0,[0,[]]],["_code","",[""]]];if(_className isEqualTo""||_position isEqualTo[]) exitWith{};private _vehicle=if(isServer&&isDedicated) then{_className createVehicle[random 100,random 100,random 100];}else{if(_code isEqualTo"") then{createSimpleObject[_className,[random 100,random 100,random 100],true];}else{_className createVehicleLocal[random 100,random 100,random 100];};};waitUntil{!isNull _vehicle};clearBackpackCargo _vehicle;clearWeaponCargo _vehicle;