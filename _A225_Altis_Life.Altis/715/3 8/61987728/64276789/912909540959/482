}else{["You do not have a blindfold.","basic"]spawn life_fnc_handleMessage;};};case 2:{if!(((goggles _targetPlayer) isEqualTo"G_blindfold_01_black_F")||((goggles _targetPlayer) isEqualTo"G_blindfold_01_white_F")) exitWith{};if([true,"blindfold",1]call life_fnc_handleInv) then{[[2,player],"life_fnc_blindfoldClient",_targetPlayer,false,false]spawn life_fnc_MP;}else{["You do not have room to hold the blindfold.","basic"]spawn life_fnc_handleMessage;};};};