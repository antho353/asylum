};case(1):{if(isNil"life_lastSynced_restartQuest") then{life_lastSynced_restartQuest=[0,[],[]]};if(life_lastSynced_restartQuest isEqualTo[life_activeQuest,life_questProgress,life_completedQuests]) exitWith{};life_lastSynced_restartQuest=+[life_activeQuest,life_questProgress,life_completedQuests];player setVariable["ga_restartQuestData",[life_activeQuest,life_questProgress,life_completedQuests],2];};};