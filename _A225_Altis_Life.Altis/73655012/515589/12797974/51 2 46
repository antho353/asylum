["Achievement",[format["Achievement Unlocked: %1",_title]]]call BIS_fnc_showNotification;};}else{if(playerSide in _sides&&(!(call _condition)&&!(_condition isEqualTo{false}))) then{life_achievements deleteAt(life_achievements find _id);if(player getVariable["ga_prefix",""]isEqualTo _title) then{player setVariable["ga_prefix",nil,true];if(player getVariable["ga_prefix",""]isNotEqualTo _preTitle) then{[15]call life_fnc_ClupdatePartial;};};};};}forEach life_achievementInfo;private _postAchieve=str life_achievements;if(_preAchieve isNotEqualTo _postAchieve) then{[7]call life_fnc_ClupdatePartial;};